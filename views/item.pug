if item.head
  p !{item.head}
table.noline
  if item.choices && item.choices.length>0
    - var choicename=pset.code+"."+(itemindex+1)
    - var symbols=pset.stdans[itemindex].ans.match(/[A-Z]/)?"ABCDEFGHIJKLMNOPQRSTUVWXYZ":"1234567890"
  each obj,index in item.choices
    tr
      td.noline(valign="top") 
        if pset.stdans[itemindex].type=="S"  
          input(type='radio' name=choicename title=choicename value=symbols[index])
        else
          input(type='checkbox' name=choicename title=choicename value=symbols[index])
      td.noline(align="left") !{obj}
//if item.spaces && item.spaces>0
//  -var leadings=item.leadings
//  -var labels=item.labels
//  include spaces
if item.tail
  p !{item.tail}
